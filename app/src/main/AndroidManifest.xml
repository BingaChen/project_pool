<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cqf.project_pool">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:name=".MyApp"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.MainActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.AlertDialogActivity" />
        <activity android:name=".ui.PopupWindowActivity" />
        <activity android:name=".ui.start.AActivity">
        </activity>
        <activity android:name=".ui.start.BActivity" />
        <activity android:name=".ui.start.CActivity" />
        <activity android:name=".ui.SetLanguageActivity"></activity>
    </application>

</manifest>